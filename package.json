{
  "name": "immut",
  "version": "0.0.1",
  "description": "Immutable datastructures",
  "main": "index.js",
  "scripts": {
    "ava": "ava",
    "build": "rollup -c && cp src/flowstub.js dist/index.js.flow && cp src/flowstub.js dist/index.es2015.js.flow",
    "build-prod": "cross-env NODE_ENV=production rollup -c",
    "prepublish": "npm run build-prod",
    "watch": "nodemon -w src -w tests -q -x \"npm run --silent test || true\"",
    "flow": "flow check .",
    "test": "flow check . && ava",
    "coverage": "nyc ava",
    "coverage-html": "nyc --reporter=html ava"
  },
  "author": "Martin Wernst√•l <m4rw3r@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.46",
    "@babel/plugin-syntax-flow": "^7.0.0-beta.46",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/register": "^7.0.0-beta.46",
    "ava": "^1.0.0-beta.4",
    "benchmark": "^2.1.4",
    "cross-env": "^5.1.4",
    "flow-bin": "^0.71.0",
    "hamt": "^2.2.2",
    "immutable": "^4.0.0-rc.9",
    "microtime": "^2.1.7",
    "nodemon": "^1.17.3",
    "nyc": "^11.7.1",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^4.0.0-beta.2",
    "rollup-plugin-gzip": "^1.3.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-watch": "^4.3.1"
  },
  "ava": {
    "require": [
      "./tests/_register.js"
    ],
    "babel": {
      "testOptions": {
        "plugins": [
          "@babel/plugin-transform-flow-strip-types"
        ]
      }
    }
  }
}
